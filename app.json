{
  "expo": {
    "name": "FitCommit",
    "slug": "FitCommit",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splashscreen_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#6B73FF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fitcommitai.fitcommit",
      "buildNumber": "9",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fitcommitai.fitcommit"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "scheme": "fitcommit",
    "extra": {
      "eas": {
        "projectId": "103198c4-16a7-4e73-b5d1-210dc4df97cb"
      }
    },
    "owner": "mur3"
  }
}
